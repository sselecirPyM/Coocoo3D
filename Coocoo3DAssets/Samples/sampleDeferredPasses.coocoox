    {
      "Name": "ShadowMapPass0",
      "ClearDepth": "true",
      "DepthBias": "3000",
      "SlopeScaledDepthBias": "1.0",
      "DepthStencil": "_ShadowMap0"
    },
    {
      "Name": "GBufferPass",
      "ClearDepth": "true",
      "DepthStencil": "_ScreenDepth0",
      "RenderTargets": [
        "_ScreenColor0",
        "_ScreenColor1",
        "_ScreenColor2",
        "_ScreenColor3"
      ]
    },
    {
      "Name": "RayTracingPass",
      "Type": "RayTracing",
      "ClearRenderTarget": true,
      "RenderTargets": [
        "_RayTracing0"
      ]
