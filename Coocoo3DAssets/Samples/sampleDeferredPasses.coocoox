    {
      "Name": "DenoisePass",
      "RenderTargets": [
        "_ScreenColor2"
      ]
    },
    {
      "Name": "DeferredFinalPass",
      "RenderTargets": [
        "_Result"
      ]
    },
    {
      "Name": "DrawTransparentPass",
      "DepthStencil": "_ScreenDepth0",
      "RenderTargets": [
        "_Result"
      ]
    },
    {
      "Name": "BloomBlur1",
